<h1>Listing posts</h1>

<table>
  <tr>
    <th>Post</th>
    <th>Course</th>
    <th>Title</th>
    <th>Desc</th>
    <th>File loc</th>
    <th>Raiting</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.post_id %></td>
    <td><%= post.course_id %></td>
    <td><%= post.title %></td>
    <td><%= post.desc %></td>
    <td><%= post.file_loc %></td>
    <td><%= post.raiting %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
